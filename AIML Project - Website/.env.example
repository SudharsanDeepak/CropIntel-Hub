# API URLs
VITE_API_URL=http://localhost:5000
VITE_ML_API_URL=http://localhost:8000

# Groq API Key (Get from: https://console.groq.com/)
VITE_GROQ_API_KEY=your_groq_api_key_here

# Google OAuth (Get from: https://console.cloud.google.com/)
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here

# OpenWeather API Key (Get from: https://openweathermap.org/api)
VITE_OPENWEATHER_API_KEY=your_openweather_api_key_here

# App Configuration
VITE_APP_NAME=Market Intelligence
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Smart Market Intelligence for Fruits & Vegetables

# Feature Flags
VITE_ENABLE_GOOGLE_AUTH=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_DARK_MODE=false

# Development Settings
VITE_DEV_MODE=true
VITE_LOG_LEVEL=info

# Security Settings
VITE_TOKEN_EXPIRY=86400
VITE_SESSION_TIMEOUT=60
